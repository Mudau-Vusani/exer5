#include <iostream>
#include <c.math>
using spacename std;
int main()
{
double distance, x1, x2, y1, y2;

cout <<"Enter first poit x1 and y1: ";
cin >> x1 >> y1;

cout <<" Enter second point x2 and y2: ";
cin >> x2 >> y2

distance = sqrt pow(pow((x1 - y1),2)+ pow((x2 - y2),2) )
cout << " Distance between two points is: "<< distance;
